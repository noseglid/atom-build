{
  "name": "build",
  "main": "./lib/build",
  "version": "0.47.0",
  "description": "Build your current project, directly from Atom",
  "repository": "https://github.com/noseglid/atom-build",
  "license": "MIT",
  "engines": {
    "atom": ">=0.50.0"
  },
  "dependencies": {
    "ansi_up": "^1.3.0",
    "atom-space-pen-views": "^2.0.3",
    "getmac": "^1.0.7",
    "lodash": "^3.8.0",
    "node-uuid": "^1.4.3",
    "tree-kill": "^0.1.1",
    "xregexp": "^2.0.0",
    "cson-parser": "^1.2.0"
  },
  "devDependencies": {
    "fs-extra": "^0.23.0",
    "temp": "^0.8.1",
    "atom-build-spec-helpers": "^0.1.0",
    "jshint": "^2.5.11",
    "jscs": "^1.9.0"
  },
  "consumedServices": {
    "builder": {
      "versions": {
        "^1.0.0": "consumeBuilder"
      }
    },
    "linter-plus-self": {
      "versions": {
        "0.1.0": "consumeLinter"
      }
    }
  },
  "scripts": {
    "test": "./node_modules/.bin/jscs lib/*.js spec/*.js && ./node_modules/.bin/jshint lib/*.js spec/*.js"
  },
  "keywords": [
    "build",
    "compile",
    "gulp",
    "make",
    "productivity"
  ]
}
